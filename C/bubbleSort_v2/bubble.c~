#include <stdio.h>

void swap(int *xp, int *yp){
  int temp=*xp;
  *xp=*yp;
  *yp=temp;
}

void printArray(int array[], int len){
  int i;
  for(i=0; i<len; i++){
    printf("%d\n", array[i]);
  }
}

void bSort(int array[], int len){
  int i;
  int j;

  for(int i=0;i<len;i++){
    for(int j=0;j<i-1;j++){
      if(array[j]<array[i]){
        swap(&array[i], &array[j]);
      }
    }
  }
}

int getLen(int array[]){
  int len=sizeof(array)/sizeof(int);
  return len;
}

int main(void){
  int arr[]={29, 72, 13, 19, 28, 22};
  int len=getLen(arr);
  bSort(arr, len);
  printArray(arr, len);
}
